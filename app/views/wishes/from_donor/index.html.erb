<h3><%= t("wishes.from_donor.views.index.header") %></h3>

<div id="donees_list" class="lists">
	<ul>
    <%= render( partial: "donee_shortcut", collection: @wishes_by_donees.donees, as: 'donee') %>
  </ul>
</div>

<div id="wishes">
  <div id="donor_wishes_list" class="lists">
    <ul class="mdl-list">
      <%= render( partial: "donee_wishes", collection: @wishes_by_donees.all) || t("wishes.from_donor.views.index.no_wishes") %>
    </ul>
  </div>
</div>



